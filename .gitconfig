[user]
	name = Caleb Spare
	email = cespare@gmail.com
[core]
  excludesfile = /Users/caleb/.gitignore
  editor = vim
[diff]
	tool = vimdiff
[color]
  diff = auto
  status = auto
  branch = auto
[alias]
  stat = status
  s = status
  c = commit
  a = add
  sn = status --untracked=no
  co = checkout
  wc = whatchanged
  l = log
  b = branch
  sub = submodule
[branch]
	autosetuprebase = always
[gc]
	auto = 0
[push]
	default = tracking
